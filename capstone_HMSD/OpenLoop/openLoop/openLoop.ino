      }
    else if(pid.ActualPressure<10){
        volt=175;
        Serial.print(pid.ActualPressure,4);
      Serial.print(",");
      Serial.print(volt/255*30,4);
      Serial.println();
        analogWrite(pwm, volt);
        delay(runtime);
        analogWrite(pwm, 0);
        }
    else if(pid.ActualPressure<13){
      volt=200;
      Serial.print(pid.ActualPressure,4);
      Serial.print(",");
      Serial.print(volt/255*30,4);
      Serial.println();
      analogWrite(pwm, volt);
      delay(runtime);
      analogWrite(pwm, 0);
      }
    else{
      volt=225;
      Serial.print(pid.ActualPressure,4);
      Serial.print(",");
      Serial.print(volt/255*30,4);
      Serial.println();
      analogWrite(pwm, volt);
      delay(runtime);
    }
  }
  else if(diff<0){
    digitalWrite(In1,LOW);
    digitalWrite(In2,HIGH);
    if(diff>-3){
      volt=0;
      Serial.print(pid.ActualPressure,4);
      Serial.print(",");
      Serial.print(volt/255*30,4);
      Serial.println();
      analogWrite(pwm, volt);
      delay(runtime);
      analogWrite(pwm, 0);
    }
    else if(pid.ActualPressure>-7){
      volt=150;
      Serial.print(pid.ActualPressure,4);
      Serial.print(",");
      Serial.print(volt/255*30,4);
      Serial.println();
      analogWrite(pwm, volt);
      delay(runtime);
      analogWrite(pwm, 0);
      }
    else if(pid.ActualPressure>-10){
        volt=175;
        Serial.print(pid.ActualPressure,4);
      Serial.print(",");
      Serial.print(volt/255*30,4);
      Serial.println();
        analogWrite(pwm, volt);
        delay(runtime);
        analogWrite(pwm, 0);
        }
    else if(pid.ActualPressure>-13){
      volt=200;
      Serial.print(pid.ActualPressure,4);
      Serial.print(",");
      Serial.print(volt/255*30,4);
      Serial.println();
      analogWrite(pwm, volt);
      delay(75);
      analogWrite(pwm, 0);
      }
    else{
      volt=225;
      Serial.print(pid.ActualPressure,4);
      Serial.print(",");
      Serial.print(volt/255*30,4);
      Serial.println();
      analogWrite(pwm, volt);
      delay(runtime);
    }
  }
    temp=pid.ActualPressure;

  //analogWrite(pwm, volt);
  //delay(10)

//filter test
  /*if (millis() > refresh_time) {
