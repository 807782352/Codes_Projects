#define In1 7
#define In2 8
#define pwm 10
#define Flex A0
#define Pressure A1
#define runtime 125

float Pread, Fread, Pkal, Fkal;
float temp=0;
SimpleKalmanFilter simpleKalmanFilter(0.1,0.1,0.01);
const long SERIAL_REFRESH_TIME = 100;
long refresh_time;
float volt=2;

struct _pid{
  float SetPressure;
  float ActualPressure;
  float err;
  float err_next;
  float err_last;
  float Kp,Ki,Kd;
  }pid;

void setup()
{
  //delay(1000);
  PID_init();


  Serial.begin(9600);
  pinMode(In1, OUTPUT);
  pinMode(In2, OUTPUT);
  
}

