    Serial.print(",");
    Serial.print(Pkal,4);
    Serial.println();
    
    refresh_time = millis() + SERIAL_REFRESH_TIME;
  }*/
}
void readsensor()
{
  Pread= analogRead(Pressure)*100/1024;
  if(Pread<0) Pread=31;
  Fread= analogRead(Flex)*100/1024;
}

void filter()
{
  Pkal=simpleKalmanFilter.updateEstimate(Pread);
  Fkal=simpleKalmanFilter.updateEstimate(Fread);
}



void PID_init(){
  pid.SetPressure=0;
    pid.ActualPressure=0;
    pid.err=0.0;
    pid.err_last=0.0;
    pid.err_next=0.0;
    pid.Kp=0.2;
    pid.Ki=0.5;
    pid.Kd=0;
  }

float PID_realize(float p){
    pid.SetPressure=p;
    pid.err=pid.SetPressure-pid.ActualPressure;
    volt=5*(pid.Kp*(pid.err-pid.err_next)+pid.Ki*pid.err+pid.Kd*(pid.err-2*pid.err_next+pid.err_last));
    if(volt>255) volt=255;
    else if(volt<0) volt=0;
    //pid.ActualPressure+=(pid.Kp*(pid.err-pid.err_next)+pid.Ki*pid.err+pid.Kd*(pid.err-2*pid.err_next+pid.err_last));
    pid.err_last=pid.err_next;
    pid.err_next=pid.err;
    return volt;
}
